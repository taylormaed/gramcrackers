<h1>Words</h1>
<table class="table">
<thead class="thead-inverse">
	<th>Text</th>
	<th>Letters</th>
</thead>
<tbody>
	<% @words.each do |word| %>
	<tr>
	<td style="text-align: center">
		<a href="<%= "/words/#{word.id}" %>"><%= word.text %></a></td>
		<td style="text-align: center"><%= word.letters %></td>

	<% end %>
	</tbody>
</table>